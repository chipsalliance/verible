{
    "problemMatcher": [
        {
            "owner": "symbiflow-checks",
            "pattern": [
                {
                    "regexp": "^:([^ ]+) file=([^,]+),line=([^,]),col=([^:]+):(.*)$",
		    "severity": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4,
		    "message": 5
                }
            ]
        }
    ]
}
